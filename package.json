{
  "name": "aolausoro.tech",
  "jest": {
    "verbose": true,
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      ".+\\.(css|less|scss|png|jpg)$": "identity-obj-proxy"
    }
  },
  "version": "1.0.0",
  "description": "portfolio website",
  "main": "server",
  "scripts": {
    "dev": "concurrently \"npm run watch:css\" \"npm run clean\" \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "parcel public/index.html --out-dir development -p 1234",
    "dev:server": "nodemon --watch ./server",
    "build:client": "npm run build:css && parcel build public/index.html --out-dir dist --public-url ./",
    "clean": "rimraf ./development && rimraf -rf ./.cache",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "test": "jest --watch"
  },
  "author": "Adewoyin Oladipupo-Usoro",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime-corejs2": "^7.9.6",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "concurrently": "^5.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^7.1.1",
    "postcss-modules": "^2.0.0",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "axios": "^1.6.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chokidar": "3.0.0",
    "config": "^3.3.1",
    "core-js": "^3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.4",
    "express-validator": "^6.4.1",
    "firebase": "^7.14.2",
    "firebase-admin": "^8.12.0",
    "framer-motion": "^2.7.7",
    "fsevents": "^2.1.3",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.6",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-recaptcha": "^2.0.1",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.7",
    "request": "^2.88.2",
    "styled-components": "^5.1.0",
    "tailwindcss": "^1.8.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "since 2017-06",
    "last 3 and_chr versions",
    "last 3 chrome versions",
    "last 3 opera versions",
    "last 3 ios_saf versions",
    "last 3 safari versions"
  ]
}
